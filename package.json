{
  "name": "zero-budget",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/",
    "typecheck": "tsc --noEmit",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "update:preview": "eas update --channel preview",
    "update:production": "eas update --channel production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@sentry/react-native": "~7.2.0",
    "@shopify/react-native-skia": "^2.4.14",
    "@supabase/supabase-js": "^2.93.2",
    "@tanstack/react-query": "^5.90.20",
    "date-fns": "^4.1.0",
    "expo": "~54.0.32",
    "expo-apple-authentication": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "jest-expo": "~54.0.16",
    "lucide-react-native": "^0.563.0",
    "nativewind": "^4.2.1",
    "posthog-react-native": "^4.24.1",
    "react": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.7.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-worklets": "^0.7.2",
    "tailwindcss": "^3.3.2",
    "victory-native": "^41.20.2",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.2"
  },
  "private": true
